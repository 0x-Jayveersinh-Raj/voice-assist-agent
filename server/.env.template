##### Server environment template
# Copy this file to server/.env and fill values as needed.

# Deepgram (STT) API key (if using Deepgram streaming)
DEEPGRAM_API_KEY=
DEEPGRAM_MODEL=nova-2

# LLM: Gemini API key
GEMINI_API_KEY=

# Default LLM provider name (e.g. gemini, other)
LLM=gemini

# Gemini HTTP API URL (set to your vendor/endpoint)
GEMINI_API_URL=

# STT default language
STT_LANGUAGE=en
